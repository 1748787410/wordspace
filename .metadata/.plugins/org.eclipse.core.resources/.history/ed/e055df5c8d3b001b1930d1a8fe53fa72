package day10;

public class Test2Abstract {

	public static void main(String[] args) {
		
		Fu2 f = new zi2();
		f.name = "小芳";
		System.out.println(f.name);
	}
}
abstract class Fu2{
	//抽象类里可以有--构造函数
	//构造方法一般用来new，抽象类不能new
	//
	public Fu2() {
		System.out.println(1);
	}
	String name = "XX";
} 
class zi2 extends Fu2{
	public zi2() {
		super();
		System.out.println(2);
	}
}