package Xltm;

import java.util.Scanner;

public class ForCycle {

	public static void main(String[] args) {
		
		forcycle();
	}

	private static void forcycle() {
		int i = new Scanner(System.in).nextInt();
		for( i = 1; i <= 10; i++) {
			for(int j = 1; j <= i; j++) {
				System.out.println("*");
			}
		}
		
	}
}
