package Xltm;

import java.util.Scanner;

public class BubbleSort {

	public static void main(String[] args) {
		int [] a = new int [8];
		System.out.println("输入随机八个数进行比较：");
		a[8] = new Scanner(System.in).nextInt();
		
		for(int i = 0; i<a.length-1; i++) {
			for(int j = 0; j<a.length-1; j++) {
				if(j<a[j]+1) {
					int t=j;
					j = a[j]+1;
					a[j]+1 =t;
				}
					
				
			}
		}
	}
}