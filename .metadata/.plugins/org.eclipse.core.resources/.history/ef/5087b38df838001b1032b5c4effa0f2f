package day07;

public class Test1 {

	public static void main(String[] args) {
		
		method();
	}

	private static void method() {
		//1.定义数组
		int [] a = {93,56,46,65,53};
		//2.遍历获取每个数据
		int count = 0;//定义变量，记录偶数个数
		for(int i = 0; i<a.length; i++) {
			//3.判断是不是偶数
			if(a[i]%2==0) {
				//4.如果是偶数++
				count++;
			}
		}
		System.out.println(count);
		
	}
}
