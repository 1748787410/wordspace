package Xltm;

import java.util.Scanner;

public class ForCycle {

	public static void main(String[] args) {
		
		forcycle();
	}

	private static void forcycle() {
		
		for( int i = 0; i <= 10; i++) {
			System.out.println();
			for(int j = 0; j <= i; j++) {
				System.out.println("*");
			}
		}
		
		
	}
}
